{
  "manifest_version": 2,
  "version": "3.0.0",
  "name": "Felica IDm Reader App for RC-S380",
  "description": "Reading Felica Card IDm, writing user ID and Key, without native applications",
  "minimum_chrome_version": "23",
  "offline_enabled": true,
  "app": {
    "background": {
      "scripts": ["jquery.min.js", "tripledes.js", "functionsLib.js", "functionsLib2.js", "IDmReaderApp.js"],
      "persistent": true
    }
  },
  "permissions": [
      "usb",
      { "usbDevices": [{"vendorId": 1356, "productId": 1731 }, {"vendorId": 1356, "productId": 1729 }] }
   ]
}
